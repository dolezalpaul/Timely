@section scripts{
    <script type="text/javascript">
        (function () {
            window.User = Ember.Object.create({
                name: '@User.Identity.Name',
                isAuthenticated: @(User.Identity.IsAuthenticated.ToString().ToLower()),
                isAdministrator: @(User.IsInRole("Administrator").ToString().ToLower())
            });
        })();
    </script>
}